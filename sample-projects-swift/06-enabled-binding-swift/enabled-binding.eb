entity MyRootEntity {
  property Bool docBool default yes ;
}

#--------------------------------------------------------------*

prefs MyPrefs {
  property Bool prefBoolean default yes ;
  outlet PMSwitch prefBoolCheckBox $value self.prefBoolean ;
}

#--------------------------------------------------------------*

document MyDocument root MyRootEntity {
  outlet PMSwitch docBoolCheckBox $value root.docBool ;
  outlet PMButton myButton $enabled ! root.docBool & MyPrefs.prefBoolean ;
}

#--------------------------------------------------------------*

