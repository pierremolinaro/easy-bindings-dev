entity MyRootEntity class MyEntityClass {
  attribute NSString docString default "doc string" ;
  transient NSString transientConcatString dependsFrom self.docString, PMPrefs.myPrefString, PMPrefs.prefTransientString ;
  transient NSString otherTransientConcatString dependsFrom PMPrefs.myPrefString ;
}

#--------------------------------------------------------------*

preferences PMPrefs {
  attribute NSString myPrefString default "pref string" ;
  outlet NSTextField myPrefStringTextField $value self.myPrefString ;
  transient NSString prefTransientString dependsFrom self.myPrefString ;
}

#--------------------------------------------------------------*

document PMDocument root MyRootEntity {
  outlet NSTextField docStringTextField $value root.docString ;
  outlet NSTextField prefStringTextField $value PMPrefs.myPrefString ;
  outlet NSTextField prefTransientStringTextField $value PMPrefs.prefTransientString ;
  outlet NSTextField transientConcatStringTextField $value root.transientConcatString ;
}

#--------------------------------------------------------------*

end
