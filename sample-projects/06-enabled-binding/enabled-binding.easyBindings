entity MyRootEntity {
  attribute bool docBool default YES ;
}

#--------------------------------------------------------------*

preferences MyPrefs {
  attribute bool prefBoolean default YES ;
  outlet NSButton prefBoolCheckBox $value self.prefBoolean ;
}

#--------------------------------------------------------------*

document MyDocument root MyRootEntity {
  outlet NSButton docBoolCheckBox $value root.docBool ;
  outlet NSButton myButton $enabled ! root.docBool & MyPrefs.prefBoolean ;
}

#--------------------------------------------------------------*

end
