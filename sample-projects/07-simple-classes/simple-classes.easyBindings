class MyClass {
  attribute bool myBool default YES ;
  attribute integer myInteger default 123 ;
  attribute NSString myString default "555" ;
  
  transient NSString schmurtz dependsFrom self.myInteger, self.myString ;
  transient NSString truc dependsFrom self.schmurtz ;
}

class MyOtherClass : MyClass {
  attribute bool chose default NO ;
  transient NSString bidule dependsFrom self.chose, self.schmurtz ;
}

end
