//-----------------------------------------------------------------------------*

void store%!ENUM_TYPE_NAME%InDictionary (const %!ENUM_TYPE_NAME% inValue,
                               NSString * inKey,
                               NSMutableDictionary * ioDictionary) ;

//-----------------------------------------------------------------------------*

%!ENUM_TYPE_NAME% read%!ENUM_TYPE_NAME%FromDictionary (NSString * inKey,
                                NSDictionary * inDictionary) ;

