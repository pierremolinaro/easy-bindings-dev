import Cocoa

//-----------------------------------------------------------------------------*

extension PMEntityManager {

  //---------------------------------------------------------------------------*
  //  newInstanceOfEntityNamed                                                 *
  //---------------------------------------------------------------------------*

  func newInstanceOfEntityNamed (inEntityTypeName : String) -> PMManagedObject? {
    var result : PMManagedObject?
%
foreach ENTITY_MAP
before
  %    %
do
  %if inEntityTypeName == "%!lkey->string%" {\n%
  %      result = %!lkey->string% (entityManager:self)\n%
between
  %    }else %
after
  %    }\n%
end
%    if nil != result {
      addEntity (result!)
    }
    return result
  }

}

