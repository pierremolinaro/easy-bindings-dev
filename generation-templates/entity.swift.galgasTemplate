        }
%   if mOppositeRelationshipName != "" then
      if [mInverseRelationMultiplicity isCollection] then
%      //--- Reset old opposite relation ship
        if let unwrappedOldValue = oldValue {
          unwrappedOldValue.%!mOppositeRelationshipName%_property.remove (unwrappedOwner)
        }
      //--- Set new opposite relation ship
        if let unwrappedValue = mValue {
          unwrappedValue.%!mOppositeRelationshipName%_property.add (unwrappedOwner)
        }
%      else
