        updateManagedObjectToOneRelationshipDisplay (object: propval, button:unwrappedExplorer)
      }
    }
  }
 
  weak private var mValue : %![[mProperty mRelationshipType] swiftTypeName]%? { // SOULD BE WEAK
    didSet {
