        oldValue?.%!mOppositeRelationshipName% = nil
      //--- Set new opposite relation ship
        %!mToOneRelationshipName%?.%!mOppositeRelationshipName% = self
%      end
     end
%      //--- Remove property observers of old object
%    for () in mDestinationEntityObservablePropertyMap do
       if not [mType isEntityType] then
       %        oldValue?.%!lkey.string%_property.removeEBObserversFrom (mObserversOf_%!lkey.string%)\n%
       end
     end
%      //--- Add property observers to new object
%    for () in mDestinationEntityObservablePropertyMap do
       if not [mType isEntityType] then
       %        mValue?.%!lkey.string%_property.addEBObserversFrom (mObserversOf_%!lkey.string%)\n%
       end
     end
%       //--- Notify observers
        postEvent ()
      }
    }
  }

  var propval : %![mRelationshipType swiftTypeName]%? { get { return mValue } }

  var prop : EBSelection <%![mRelationshipType swiftTypeName]%?> { get { return .single (mValue) } }

  func setProp (_ value : %![mRelationshipType swiftTypeName]%?) { mValue = value }

  //····················································································································

  @objc func performUndo (_ oldValue : %![mRelationshipType swiftTypeName]%?) {
    mValue = oldValue
  }

  //····················································································································

  func remove (_ object : %![mRelationshipType swiftTypeName]%) {
    if mValue === object {
      mValue = nil
    }
  }
  
  //····················································································································

