    self.mModel?.setProp (inArrayValue)
  }

  // - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -

  override var selection : EBSelection < [%!ENTITY_NAME%] > {
