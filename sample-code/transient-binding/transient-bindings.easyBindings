preferences Prefs {
  attribute mName : NSString default "Amédée" ;
  attribute mFirstName : NSString default "Schmurtz" ;
  transient mFullName : NSString dependsFrom .mName, .mFirstName ;
  outlet mNameTextField : NSTextField $value .mName ;
  outlet mFirstNameTextField : NSTextField $value .mFirstName ;
  outlet mFullNameTextField : NSTextField $value .mFullName ;
}

end
