entity MyPrefsEntity class MyPrefsEntityClass {
  attribute NSString myString default "hello" ;
  
  transient NSString myStringMaj dependsFrom .myString ;
  transient NSString myStringMin dependsFrom .myString ;
  transient NSString myStringConcat dependsFrom .myStringMaj, .myStringMin ;
}

