document MyDocument root MyPrefsEntity {
  outlet NSTextField myTextField $value root.myString ;
  outlet NSTextField myTextMajField $value root.myStringMaj ;
  outlet NSTextField myTextMinField $value root.myStringMin ;
  outlet NSTextField myTextConcatField $value root.myStringConcat ;
}

